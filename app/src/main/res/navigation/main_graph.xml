<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/productsFragment">

    <fragment
        android:id="@+id/productsFragment"
        android:name="com.dgparkcode.expbox.ui.products.ProductsFragment"
        android:label="ProductsFragment"
        tools:layout="@layout/fragment_products">
        <action
            android:id="@+id/action_productsFragment_to_productAdderFragment"
            app:destination="@id/productAdderFragment" />
    </fragment>
    <fragment
        android:id="@+id/productAdderFragment"
        android:name="com.dgparkcode.expbox.ui.productadder.ProductAdderFragment"
        android:label="ProductAdderFragment"
        tools:layout="@layout/fragment_product_adder" />
</navigation>